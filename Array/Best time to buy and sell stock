class Solution {
    public int maxProfit(int[] prices) {
        int buy_price=prices[0];

        int maxProfit=0;

        for(int i=1;i<prices.length;i++){
            if(prices[i]<buy_price){
                buy_price=prices[i];
            }else{
                int currentProfit=prices[i]-buy_price;
                maxProfit=Math.max(currentProfit,maxProfit);
            }
        }
        return maxProfit;
    }
}
TC:O(N)
SC:O(1)
